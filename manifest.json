{
 "manifest_version": 2,
 "name": "News reader",
 "version": "1.0",

 "permissions": [
   "tabs", "http://*/*", "https://*/*", "activeTab"
 ],
 "background": {
   "scripts": ["background.js"]
 },
 "browser_action": {
     "default_title": "Set this page's color.",
     "default_icon" : "icon.png",
     "default_popup": "popup.html"
 }

}
